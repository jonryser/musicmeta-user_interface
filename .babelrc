{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "useBuiltIns": "entry",
          "modules": false,
          "targets": {
            "chrome": "66",
            "ie": "11",
            "safari": "10.3",
            "firefox": "60",
            "node": 8
          }
        },
        "transform-runtime": {
          "corejs": false,
          "helpers": true,
          "regenerator": true,
          "useESModules": false
        },
        "class-properties": {}
      }
    ],
    "@zeit/next-typescript/babel"
  ],
  "plugins": [
    "lodash",
    "@babel/plugin-proposal-object-rest-spread",
  ]
}